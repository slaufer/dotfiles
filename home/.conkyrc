alignment bottom_right
default_color 69a4a1
color1 6dd7fe
background no
own_window no
double_buffer no
use_xft yes
xftfont sans-11
update_interval 1
use_spacer left
pad_percents 2
gap_x 4
gap_y 5

TEXT
${color1}disk $alignr${color}${fs_bar 15,100 /}
${color1}cpu $alignr${color}${cpugraph cpu0 15,100 5c94ba a0a5ed -t}
${color1}mem $alignr${color}${memgraph 15,100 5c94ba a0a5ed -t }

${color1}$alignr${time %A, %B %e, %Y}
${color1}$alignr${time %l:%M:%S %p}
